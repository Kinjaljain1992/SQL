#Write the query to create a database named 'Company'
CREATE DATABASE Company;

#Write the query to use the database 'Company'
USE Company;

#Write the query to create a table named 'employees'
CREATE TABLE employees(
empid INT PRIMARY KEY,
empName VARCHAR(50),
empAge INT,
empAddress VARCHAR(50),
empSalary INT 
);

#Write the query to create database  named 'Cubix' and table 'users'-

CREATE DATABASE Cubix;

USE Cubix;

CREATE TABLE users
(
id int,
name varchar(100),
dob date,
gender enum("M","F","O"),
email varchar(150),
password varchar(50),
contact int,
address varchar(100),
city varchar(50),
state varchar(50)
);
